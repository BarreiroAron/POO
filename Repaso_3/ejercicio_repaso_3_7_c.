#include <stdio.h>

int main() {
    int cantidad = 0;
    printf("Ingrese la cantidad de numeros que quiere: ");
    scanf("%d", &cantidad);
    float numeros[10];
    float suma = 0.0;
    float media;
    int i;

    printf("Ingresa 10 numeros reales:\n");
    for (i = 0; i < cantidad; i++) {
        printf("Numero %d: ", i + 1);
        scanf("%f", &numeros[i]);
        suma += numeros[i];
    }

    media = suma / cantidad;
    printf("\nLa media de los numeros ingresados es: %.2f\n", media);

    printf("Los numeros que estÃ¡n por encima de la media son:\n");
    for (i = 0; i < cantidad; i++) {
        if (numeros[i] > media) {
            printf("%.2f\n", numeros[i]);
        }
    }

    return 0;
}
